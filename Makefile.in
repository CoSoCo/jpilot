dist: clean
	@echo
	@echo "->Copying all release files to the directory " $(PACKAGE)-$(VERSION)
	@echo
	/bin/rm -f $(PACKAGE)-$(VERSION).tar.gz
	-mkdir $(PACKAGE)-$(VERSION)
	tar cf - $(SRC) | (cd $(PACKAGE)-$(VERSION); tar xf -)
	/bin/rm -rf $(PACKAGE)-$(VERSION)/intl/CVS/
	/bin/rm -rf $(PACKAGE)-$(VERSION)/po/CVS/
	/bin/rm -rf $(PACKAGE)-$(VERSION)/Expense/CVS/
	/bin/rm -rf $(PACKAGE)-$(VERSION)/KeyRing/CVS/
	/bin/rm -rf $(PACKAGE)-$(VERSION)/SyncTime/CVS/
	/bin/rm -rf $(PACKAGE)-$(VERSION)/docs/CVS/
	/bin/rm -rf $(PACKAGE)-$(VERSION)/empty/CVS/
	/bin/rm -rf $(PACKAGE)-$(VERSION)/m4/CVS/
	/bin/rm -rf $(PACKAGE)-$(VERSION)/icons/CVS/
	@echo
	@echo "->Making the compressed tar file " $(PACKAGE)-$(VERSION).tar.gz
	@echo
	tar cf - $(PACKAGE)-$(VERSION) | gzip > $(PACKAGE)-$(VERSION).tar.gz
	@echo
	@echo "->Removing the temporary directory " $(PACKAGE)-$(VERSION)
	/bin/rm -rf $(PACKAGE)-$(VERSION)
		
rpm: dist
	rpm -tb $(PACKAGE)-$(VERSION).tar.gz

srpm: dist
	rpm -ts $(PACKAGE)-$(VERSION).tar.gz
